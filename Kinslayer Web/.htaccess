RewriteRule ^elements-of-a-room(.*)$		/? [L,R=301]
RewriteRule ^equipment-listing/armor/(.*)-(.*)$	./EquipmentCatalog.do?EquipmentListingType=0&ObjectWearType=$2 [QSA,L]
