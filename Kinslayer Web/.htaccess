RewriteRule ^getting-started(.*)$		./getting_started.php$1 [QSA,L]

RewriteRule ^index.php(.*)$	/$1 [QSA,L,R=301]
RewriteRule ^introduction-to-mining(.*)$	./TradesMining.do$1 [QSA,L]
RewriteRule ^introduction-to-smithing(.*)$	./TradesSmithing.do$1 [QSA,L]
RewriteRule ^introduction-to-equipment(.*)$	./EquipmentIntroduction.do$1 [QSA,L]

RewriteRule ^help.php(.*)$			/? [L,R=301]

