RewriteRule ^mud-clients(.*)$			/? [L,R=301]
RewriteRule ^moving-around(.*)$			/? [L,R=301]

RewriteRule ^namegen.php(.*)$			/? [L,R=301]
RewriteRule ^new-to-muds(.*)$			/? [L,R=301]
